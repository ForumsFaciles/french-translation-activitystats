	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Robert Heim",
			"homepage": "http://blog.robert-heim.de",
			"email": "robert@robert-heim.de",
			"role": "Lead Developer"
		}
	],
	"require": {
		"php": ">=5.3.3"
	},
	"extra": {
		"display-name": "Activity Stats",
		"soft-require": {
			"phpbb/phpbb": "3.1.*"
